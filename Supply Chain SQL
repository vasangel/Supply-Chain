    -- QUERIES
    
SELECT * FROM Suppliers; 
SELECT * FROM Transportationfees; 
SELECT * FROM Orders; 
SELECT Country,COUNT(OrderID) AS Count FROM OrderDetails
GROUP BY Country
ORDER BY Count DESC;


-- Most Transportations
SELECT S.SupplierName,COUNT(O.OrderID) AS Transportations 
FROM Suppliers S 
JOIN Orders AS O
ON S.SupplierID = O.SupplierID
GROUP BY S.SupplierName
ORDER BY Transportations DESC;

-- Most Money paid per Company
SELECT Suppliers.SupplierName,
SUM((Transportationfees.PackageMaxWeight * Transportationfees.Price)) AS Total_Price
FROM Suppliers 
JOIN Orders ON Suppliers.SupplierID = Orders.SupplierID
JOIN Transportationfees ON FeeID = Orders.FeePackageID
GROUP BY Suppliers.SupplierName
ORDER BY Total_Price DESC;
 
 -- Most shippments
SELECT Country,COUNT(OrderID) AS Count FROM OrderDetails
GROUP BY Country
ORDER BY Count DESC;

-- Most Packages

SELECT Transportationfees.PackageName,COUNT(Orders.FeePackageID) AS Packages
FROM Transportationfees
JOIN Orders ON
Transportationfees.FeeID = Orders.FeePackageID
GROUP BY Transportationfees.PackageName,Transportationfees.Price
ORDER BY Packages DESC;

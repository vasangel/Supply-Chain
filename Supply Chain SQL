CREATE TABLE TransportationFees (
FeeID INT PRIMARY KEY auto_increment,
PackageName VARCHAR(100),
PackageMaxWeight INT,
Price INT);

INSERT INTO TransportationFees(PackageName,PackageMaxWeight,Price)
VALUES
("Junior Package",100,200),
("Basic Package",500,980),
("Intermediate Package",1000,1900),
("OneQuarter Truck Package",5000,9000),
("HalfTruck Package",10000,17000),
("All Inclusive Package",20000,30000);



CREATE TABLE Orders (
    OrderID INT PRIMARY KEY auto_increment,
    SupplierID INT,
    FeePackageID INT,
    OrderDate DATE,
    ExpectedDelivery DATE,
    Delivered DATE,
    FOREIGN KEY (SupplierID) REFERENCES Suppliers(SupplierID),
    FOREIGN KEY (FeePackageID) REFERENCES TransportationFees(FeeID)
);

